<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/y71"
    android:background="#bfbfbf"
    android:orientation="horizontal">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y70"
        android:background="#ffffff"
        android:orientation="horizontal"
        >
    <TextView
        android:id="@+id/tvItemFixedFlowDataId"
        android:layout_width="@dimen/x70"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textSize="@dimen/x24"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/tvItemFixedFlowDataCreateTime"
        android:layout_width="@dimen/x300"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textSize="@dimen/x24"
        android:textColor="#000000"
        android:text="2018-3-13 15:49:04" />

    <TextView
        android:id="@+id/tvItemFixedFlowName"
        android:layout_width="@dimen/x200"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textSize="@dimen/x24"
        android:textColor="#000000"
        android:text="定点" />

    <TextView
        android:id="@+id/tvItemFixedFlowDataNowShift"
        android:layout_width="@dimen/x191"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textSize="@dimen/x24"
        android:textColor="#000000"
        android:text="2.00" />

    <TextView
        android:id="@+id/tvItemFixedFlowDataShift"
        android:layout_width="@dimen/x191"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textSize="@dimen/x24"
        android:textColor="#000000"
        android:text="2.00" />

    <TextView
        android:id="@+id/tvItemFixedFlowDataAddShift"
        android:layout_width="@dimen/x191"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textSize="@dimen/x24"
        android:textColor="#000000"
        android:text="2.00" />

    <TextView
        android:id="@+id/tvItemFixedFlowDataObd"
        android:layout_width="@dimen/x191"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textSize="@dimen/x24"
        android:textColor="#000000"
        android:text="3.74600" />
    </LinearLayout>
</LinearLayout>